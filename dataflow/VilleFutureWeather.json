{
	"name": "VilleFutureWeather",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DimDate",
						"type": "DatasetReference"
					},
					"name": "DimDate"
				},
				{
					"dataset": {
						"referenceName": "DimRegion",
						"type": "DatasetReference"
					},
					"name": "DimRegion"
				},
				{
					"dataset": {
						"referenceName": "WeatherMetrics",
						"type": "DatasetReference"
					},
					"name": "WeatherMetrics"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dimDateCleaned",
						"type": "DatasetReference"
					},
					"name": "DimDateCleaned"
				},
				{
					"dataset": {
						"referenceName": "dimRegionCleaned",
						"type": "DatasetReference"
					},
					"name": "DimRegionCleaned"
				},
				{
					"dataset": {
						"referenceName": "WeatherMetricsCleaned",
						"type": "DatasetReference"
					},
					"name": "WeatherMetricsCleaned"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          DateID as string,",
				"          Date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimDate",
				"source(output(",
				"          RegionID as string,",
				"          RegionName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimRegion",
				"source(output(",
				"          DateID as string,",
				"          RegionID as string,",
				"          AverageTemperature as string,",
				"          Humidity as string,",
				"          WindSpeed as string,",
				"          Rainfall as string,",
				"          SunshineHours as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> WeatherMetrics",
				"DimDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['DimDateCleaned.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DimDateCleaned",
				"DimRegion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['DimRegionCleaned.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DimRegionCleaned",
				"WeatherMetrics sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['WeatherMetricsCleaned.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> WeatherMetricsCleaned"
			]
		}
	}
}